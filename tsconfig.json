{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,
    "noImplicitAny": false,
    "outDir": "dist/",
    "allowSyntheticDefaultImports": true
  },
  
"include": [
  "src/**/*"
],
"exclude": [
]
}